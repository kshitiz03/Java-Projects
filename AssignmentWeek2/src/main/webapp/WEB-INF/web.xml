<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>LoginController</servlet-name>
		<display-name>LoginController</display-name>
		<description></description>
		<servlet-class>com.nagarro.Controller.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImageController</servlet-name>
		<display-name>ImageController</display-name>
		<description></description>
		<servlet-class>com.nagarro.Controller.ImageController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteController</servlet-name>
		<display-name>DeleteController</display-name>
		<description></description>
		<servlet-class>com.nagarro.Controller.DeleteController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateController</servlet-name>
		<display-name>UpdateController</display-name>
		<description></description>
		<servlet-class>com.nagarro.Controller.UpdateController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutController</servlet-name>
		<display-name>LogoutController</display-name>
		<description></description>
		<servlet-class>com.nagarro.Controller.LogoutController</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageController</servlet-name>
		<url-pattern>/ImageController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteController</servlet-name>
		<url-pattern>/DeleteController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateController</servlet-name>
		<url-pattern>/UpdateController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutController</servlet-name>
		<url-pattern>/LogoutController</url-pattern>
	</servlet-mapping>


</web-app>
